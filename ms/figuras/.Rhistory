# 4. meoization verision
fact_tbl <- c(1, rep(NA, 25))
my_factorial_mem <- function(n){
stopifnot(n > 0)
if(!is.na(fact_tbl[n])){
fact_tbl[n]
} else {
fact_tbl[n - 1] <<- my_factorial_mem(n - 1)
(n:1)[1] * my_factorial_func(n - 1)
}
}
